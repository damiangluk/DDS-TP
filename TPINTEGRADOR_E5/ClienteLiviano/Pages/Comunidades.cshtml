@page
@model IncidentesModel
@{
    ViewData["Title"] = "Home page";
}


@section Scripts {
    <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap-grid.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap-reboot.css" rel="stylesheet" />
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/comunidadesController.js" asp-append-version="true"></script>
}



<div onload="getAllComunidades()" class="container-incidentes">
    <div class="incidentes-title">
        <div class="rectangle-title">
            <span class="container-span-title"> Comunidades </span>
        </div>
    </div>
    <div class="incidentes-data">
        <div class="container-filters">
            <div class="actions-filter">
                <button class="button" onclick="getAllComunidades()">Buscar</button>
            </div>
        </div>
        <div class="container-table" id="comunidades-container">
        </div>
    </div>
</div>